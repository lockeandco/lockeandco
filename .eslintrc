{
    "extends": [
      "plugin:jest/recommended",
      "prettier/react",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "jest",
      "better"
    ],
    "env": {
      "jest": true
    },
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": "off",
      "react/no-array-index-key": "off",
      "function-paren-newline": "off",
      "comma-dangle": "off",
      "arrow-parens": "off",
      "max-len": ["error", 120,4]
    },
    "parser": "babel-eslint"
  }
  
  